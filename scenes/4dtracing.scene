[lofi]

Resolution=160,120

Iterations=500

Stepsize=0.002

[hifi]

Resolution=640,480
Iterations=500
Stepsize=0.005



[geometry]

Distort=1

Cameraposition=0,0,10.
Fourvelocity=1.,0.,0.,0.

#Tangent of field of view
Fieldofview=2.

#Self explanatory
#fixes the view vector
Lookat=0.,0.,0.

#The vector defining the "up" direction
#(it's actually projected on the orthogonal subspace to the view vector)
Upvector=0.,1.,0.


Diskinner=1.5
Diskouter=20.

[materials]

sRGBIn=0
sRGBOut=0

Disktexture=none

Skytexture=final

Horizongrid=1

Skydiskratio=0.2

Fogdo=0

Airy_bloom=1
Airy_radius=2

Blurdo=1

